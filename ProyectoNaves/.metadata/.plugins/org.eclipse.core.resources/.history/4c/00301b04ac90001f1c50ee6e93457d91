spring.application.name=planeStar

# Configuraciones de DB
spring.datasource.url=jdbc:h2:tcp://db:9092/~/test
spring.datasource.driverClassName=org.h2.Driver
spring.datasource.username=sa
spring.datasource.password=password
spring.jpa.database-platform=org.hibernate.dialect.H2Dialect
spring.h2.console.enabled=true
spring.cache.type=simple
spring.h2.console.path=/h2-console

# Configuraciones de JPA
spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true

spring.liquibase.change-log=classpath:db/changelog/db.changelog-master.xml

# custom path for api docs
springdoc.api-docs.path=/api-docs
logging.level.org.springframework.security=DEBUG



app.user.username=user
app.user.password=password
app.secret.key=clave secreta

spring.kafka.bootstrap-servers=kafka:9092
spring.kafka.consumer.group-id=ship-group
spring.kafka.consumer.auto-offset-reset=earliest